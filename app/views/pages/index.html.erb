<%= form_with url: "/pages", method: :get do |form| %>
    <%= form.label :user_id, "User ID:" %>
    <%= form.text_field :user_id %>
    <%= form.submit "Search" %>
  <% end %>
  
<% if @info %>
    <% @info.each do |img| %>
        <%= img.server %>
        <img src="//live.staticflickr.com/<%= img.server %>/<%= img.id %>_<%= img.secret %>_n.jpg" 
        <br>
    <% end %>
<% end %>

